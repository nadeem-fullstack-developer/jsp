<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from RequestHandlerClassService WSDL service -->

  <service name="RequestHandlerClass" provider="java:RPC" style="document" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://max"/>
      <parameter name="wsdlServiceElement" value="RequestHandlerClassService"/>
      <parameter name="schemaQualified" value="http://max"/>
      <parameter name="wsdlServicePort" value="RequestHandlerClass"/>
      <parameter name="className" value="max.RequestHandlerClassSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="RequestHandlerClass"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getStateDataByXML" qname="getStateDataByXML" returnQName="retNS:getStateDataByXMLReturn" xmlns:retNS="http://max" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="getXmlData" qname="getXmlData" returnQName="retNS:getXmlDataReturn" xmlns:retNS="http://max" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:xml" xmlns:pns="http://max" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getXmlData getStateDataByXML"/>

  </service>
</deployment>
